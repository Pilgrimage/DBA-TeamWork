        <Button x:Name="button2" Content="Temp Sensor" HorizontalAlignment="Left" Margin="356,26,0,0" VerticalAlignment="Top" Width="90" Height="35"/>
        <Button x:Name="button3" Content="Tension Sensor" HorizontalAlignment="Left" Margin="517,26,0,0" VerticalAlignment="Top" Width="90" Height="35"/>
        <Button x:Name="button4" Content="Volume Sensor" HorizontalAlignment="Left" Margin="676,26,0,0" VerticalAlignment="Top" Width="90" Height="35"/>

    </Grid>
</Window>
